<h1>Listing users</h1>

<table>
  <tr>
    <th>Register from</th>
    <th>User name</th>
    <th>User</th>
    <th>Uuid</th>
    <th>Membership</th>
    <th>Period</th>
    <th>Auto renewal</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.register_from %></td>
    <td><%= user.user_name %></td>
    <td><%= user.user_id %></td>
    <td><%= user.uuid %></td>
    <td><%= user.membership %></td>
    <td><%= user.period %></td>
    <td><%= user.auto_renewal %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
